<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <ImageSource x:Key="TrafficConeIcon">ms-appx:///Assets/Icons/TrafficCone-Dark.svg</ImageSource>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <ImageSource x:Key="TrafficConeIcon">ms-appx:///Assets/Icons/TrafficCone-Light.svg</ImageSource>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

    <FontFamily x:Key="HappyMonkey">ms-appx:///Assets/Fonts/HappyMonkey-Regular.ttf#Happy Monkey</FontFamily>
    <FontFamily x:Key="ShareTech">ms-appx:///Assets/Fonts/ShareTech-Regular.ttf#Share Tech</FontFamily>


</ResourceDictionary>
