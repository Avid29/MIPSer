<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <ImageSource x:Key="TrafficConeIcon">ms-appx:///Assets/Icons/TrafficCone-Dark.svg</ImageSource>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <ImageSource x:Key="TrafficConeIcon">ms-appx:///Assets/Icons/TrafficCone-Light.svg</ImageSource>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

    <!--  Logs  -->
    <ImageSource x:Key="ErrorIcon">ms-appx:///Assets/Icons/Severity/Error.svg</ImageSource>
    <ImageSource x:Key="WarningIcon">ms-appx:///Assets/Icons/Severity/Warning.svg</ImageSource>
    <ImageSource x:Key="MessageIcon">ms-appx:///Assets/Icons/Severity/Message.svg</ImageSource>
    
    <!--  Files  -->
    <ImageSource x:Key="FileIcon">ms-appx:///Assets/Icons/Files/File.svg</ImageSource>
    <ImageSource x:Key="FolderIcon">ms-appx:///Assets/Icons/Files/Folder.svg</ImageSource>
    <ImageSource x:Key="AsmFileIcon">ms-appx:///Assets/Icons/Files/asm.svg</ImageSource>
    <ImageSource x:Key="ObjFileIcon">ms-appx:///Assets/Icons/Files/obj.svg</ImageSource>

</ResourceDictionary>
