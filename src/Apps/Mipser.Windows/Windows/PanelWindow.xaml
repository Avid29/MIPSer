<?xml version="1.0" encoding="utf-8"?>
<local:MipserWindow
    x:Class="Mipser.Windows.PanelWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Mipser.Windows"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:shell="using:Mipser.Windows.Views.Shell"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource PrimaryBackgroundBrush}">
        <shell:PanelView DataContext="{x:Bind ViewModel.PanelViewModel, Mode=OneWay}"/>
    </Grid>
</local:MipserWindow>
